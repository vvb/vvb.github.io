    &lt;lsbootVirtualMedia access="read-only-local" lunId="0" mappingName="" order="2"/&gt;
    &lt;lsbootStorage order="3"&gt;
      &lt;lsbootLocalStorage&gt;
        &lt;lsbootUsbFlashStorageImage order="3"/&gt;
      &lt;/lsbootLocalStorage&gt;
    &lt;/lsbootStorage&gt;
      &lt;/lsbootPolicy&gt;
    &lt;/orgOrg&gt;
&lt;/topRoot&gt;  
</code></pre>

